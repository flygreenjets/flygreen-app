{
  "expo": {
    "name": "Flygreen",
    "slug": "flygreen-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons/ios/AppIcon@3x.png",
    "scheme": "flygreen",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.flygreen.flygreen",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": ["whatsapp"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons/android/play_store_512.png"
      },
      "package": "com.flygreen.flygreen",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icons/web/icon-192.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser",
      "react-native-map-link",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icons/android/notification-icon.png",
          "defaultChannelId": "default",
          "useNextNotificationsApi": true
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "flygreen-app",
          "organization": "flygreen"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6c7b7790-57e6-4c6a-85b2-0af558c4214d"
      }
    },
    "owner": "flygreen"
  }
}
